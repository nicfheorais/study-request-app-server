{
    "name": "study-request-app",
    "version": "1.0.0",
    "description": "gets all study requests and posts",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node ./bin/www.js",
        "dev": "nodemon -r dotenv/config  ./bin/www.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "db-drop-study-request-table": "node -r dotenv/config ./db/scripts/dropStudyRequestTable.js",
        "db-drop-user-table": "node -r dotenv/config ./db/scripts/dropUserTable.js",
        "db-create-user-table": "node -r dotenv/config ./db/scripts/createUserTable.js",
        "db-populate-user-table": "node -r dotenv/config ./db/scripts/populateUserTable.js",
        "db-create-study-request-table": "node -r dotenv/config ./db/scripts/createStudyRequestTable.js",
        "db-populate-study-request-table": "node -r dotenv/config ./db/scripts/populateStudyRequestTable.js",
        "db-create-test-table": "node -r dotenv/config ./db/scripts/createTestTable.js",
        "db-drop-test-table": "node -r dotenv/config ./db/scripts/dropTestTable.js",
        "db-deploy": "npm run db-drop-study-request-table && npm run b-drop-user-table && npm run db-create-user-table && npm run db-populate-user-table && npm run db-create-study-request-table && npm run db-populate-study-request-table"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nicfheorais/study-request-node-express-rest-api-postgresql.git"
    },
    "author": "nicfheorais",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nicfheorais/study-request-node-express-rest-api-postgresql/issues"
    },
    "homepage": "https://github.com/nicfheorais/study-request-node-express-rest-api-postgresql#readme",
    "dependencies": {
        "cors": "^2.8.5",
        "env-var": "^7.1.1",
        "express": "^4.17.2",
        "pg": "^8.7.1"
    },
    "devDependencies": {
        "dotenv": "^14.2.0",
        "nodemon": "^2.0.15",
        "jest": "^27.4.5",
        "supertest": "^6.1.6"
    }
}
